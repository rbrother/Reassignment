<Window x:Class="Reassignment.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Reassignment"
        mc:Ignorable="d"
        SizeToContent="WidthAndHeight"
        Title="MainWindow" Foreground="White" FontSize="22" FontFamily="ISOCT3" d:DesignWidth="914" d:DesignHeight="403">
    <Canvas Height="378" Width="907">
        <Image x:Name="image" HorizontalAlignment="Left" Height="378" VerticalAlignment="Top" Width="907" Source="Images/coffeeshopBeginning.png">
            <Image.OpacityMask>
                <ImageBrush ImageSource="Images/coffeeshopBeginning.png"/>
            </Image.OpacityMask>
        </Image>
        <Button x:Name="startButton" Content="Start" HorizontalAlignment="Left" Height="43" VerticalAlignment="Top" Width="116" Canvas.Left="740" Canvas.Top="158" FontWeight="Bold" Click="startClick"/>
    </Canvas>
</Window>
